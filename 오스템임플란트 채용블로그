    <script src="./images/script.js"></script>

<script>
document.addEventListener("DOMContentLoaded", function() {
  const observer = new MutationObserver(() => {
    const tags = document.querySelector('.tags');
    const likeBtn = document.querySelector('[id*="recomm_btn"], .container_postbtn');

    if (tags && likeBtn && !tags.classList.contains('moved')) {
      likeBtn.parentNode.insertBefore(tags, likeBtn);
      tags.classList.add('visible', 'moved'); // í‘œì‹œ + ì¤‘ë³µ ë°©ì§€
      observer.disconnect();
    }
  });

  observer.observe(document.body, { childList: true, subtree: true });
});
</script>

  </s_t3>
<script>
document.addEventListener("DOMContentLoaded", function() {
  document.querySelectorAll('.date').forEach(function(el) {
    const text = el.innerText.trim();
    // "2025. 10. 21. 16:01" â†’ "2025. 10. 21"
    const parts = text.split(' ');
    if (parts.length >= 3) {
      let dateOnly = parts[0] + ' ' + parts[1] + ' ' + parts[2];
      dateOnly = dateOnly.replace(/\.$/, ""); // ë§¨ ë ì  ì œê±°
      el.innerText = dateOnly;
    }
  });
});
</script>

<script>
document.addEventListener("DOMContentLoaded", function () {
  // ëª©ë¡ í˜ì´ì§€ì—ì„œë§Œ ë™ì‘ (í™ˆ, ì¹´í…Œê³ ë¦¬)
  var bodyId = document.body.id || "";
  if (!/tt-body-(index|category)/.test(bodyId)) return;

  // ì¹´ë“œë“¤ì„ ìˆœíšŒ
  document.querySelectorAll(".post-item").forEach(function(card){
    var link = card.querySelector("a[href]");
    var excerptEl = card.querySelector(".excerpt");
    if (!link || !excerptEl) return;

    // ì¼ë‹¨ ê¹”ë”í•˜ê²Œ ì´ˆê¸°í™” (ëˆˆì— ê±°ìŠ¬ë¦¬ëŠ” ê¸´ ìš”ì•½ ê°€ë¦¼)
    excerptEl.textContent = ""; 

    // ê°™ì€ ì¶œì²˜(ë„ë©”ì¸)ë¼ì„œ fetch ê°€ëŠ¥
    fetch(link.href, { credentials: "same-origin" })
      .then(function(res){ return res.text(); })
      .then(function(htmlText){
        // ë¬¸ì„œ íŒŒì‹±
        var doc = new DOMParser().parseFromString(htmlText, "text/html");
        var article = doc.querySelector("#article-view"); // ë³¸ë¬¸ ì»¨í…Œì´ë„ˆ
        if (!article) return;

        // ë³¸ë¬¸ HTML â†’ ì¤„ ì¸ì‹ ê°€ëŠ¥í•˜ê²Œ ì „ì²˜ë¦¬
        var raw = article.innerHTML
          .replace(/<\s*br\s*\/?>/gi, "\n")   // <br> â†’ ê°œí–‰
          .replace(/<\/p>\s*<p>/gi, "\n")     // ë¬¸ë‹¨ ê²½ê³„ â†’ ê°œí–‰
          .replace(/<\/div>\s*<div>/gi, "\n");

        // íƒœê·¸ ì œê±°í•˜ê³  ìˆœìˆ˜ í…ìŠ¤íŠ¸ë§Œ
        var text = raw.replace(/<[^>]*>/g, "")
                      .replace(/\r\n|\r/g, "\n")
                      .trim();

        // ì²« ë²ˆì§¸ "ë³´ì´ëŠ” ì¤„" (ë¹ˆ ì¤„ ê±´ë„ˆë›°ê¸°)
        var lines = text.split("\n").map(function(s){return s.trim();})
                        .filter(function(s){return s.length>0;});
        var firstLine = lines.length ? lines[0] : "";

        // ê²°ê³¼ ì£¼ì… (ì—†ìœ¼ë©´ ì œëª©ë§Œ ë³´ì´ê²Œ ë‘ê¸°)
        if (firstLine) {
          excerptEl.textContent = firstLine;        // ì²« ì¤„ë§Œ!
        }
      })
      .catch(function(err){
        // ì‹¤íŒ¨ì‹œ ì•„ë¬´ ê²ƒë„ í•˜ì§€ ì•ŠìŒ(ì œëª©ë§Œ ë³´ì´ê²Œ)
        // console.warn("excerpt fetch fail:", err);
        
      });
  });
});
</script>


<script>
document.addEventListener("DOMContentLoaded", function() {
  const headerSearch = document.querySelector('.header-search');
  const toggleBtn = headerSearch.querySelector('.search-toggle');
  const closeBtn = headerSearch.querySelector('.search-close');
  const searchInput = headerSearch.querySelector('.search-box input');

  toggleBtn.addEventListener('click', () => {
    headerSearch.classList.add('active');
    setTimeout(() => searchInput.focus(), 300);
  });

  closeBtn.addEventListener('click', () => {
    headerSearch.classList.remove('active');
    searchInput.value = "";
  });
});
</script>


</body>

</html>
