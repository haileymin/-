<script>
  // 모든 배너 숨기기
  document.querySelectorAll('.page-hero').forEach(el => el.style.display = 'none');

  const url = window.location.href;

  if (url.includes('/category/People')) {
    document.querySelector('.page-hero.people').style.display = 'block';
  } else if (url.includes('/category/Culture')) {
    document.querySelector('.page-hero.culture').style.display = 'block';
  } else if (url.includes('/category/News')) {
    document.querySelector('.page-hero.news').style.display = 'block';
  }
</script>


