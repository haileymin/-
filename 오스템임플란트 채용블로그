<script>
document.addEventListener("DOMContentLoaded", function() {
  const recruitBanner = document.querySelector('.recruit-banner');
  const anotherCategory = document.querySelector('.another_category');

  // 카테고리의 다른 글이 존재하면
  if (recruitBanner && anotherCategory) {
    // another_category 바로 위 요소 중, .entry-content가 있으면 그 안으로 이동
    const entryContent = document.querySelector('.entry-content');
    if (entryContent && anotherCategory.parentNode) {
      entryContent.appendChild(recruitBanner); // 본문 내부에 재배치
      // 그리고 another_category 바로 위로 이동시킴
      anotherCategory.parentNode.insertBefore(recruitBanner, anotherCategory);
    }
  }

  // 날짜 포맷 정리 (기존 코드 유지)
  document.querySelectorAll('.date').forEach(function(el) {
    const text = el.innerText.trim();
    const parts = text.split(' ');
    if (parts.length >= 3) {
      let dateOnly = parts[0] + ' ' + parts[1] + ' ' + parts[2];
      dateOnly = dateOnly.replace(/\.$/, "");
      el.innerText = dateOnly;
    }
  });
});
</script>
